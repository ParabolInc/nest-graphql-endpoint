{
  "name": "nest-graphql-endpoint",
  "author": "Matt Krick <matt.krick@gmail.com>",
  "version": "0.6.0",
  "description": "Nest an external GraphQL Endpoint",
  "repository": {
    "type": "git",
    "url": "https://github.com/parabolinc/nest-graphql-endpoint"
  },
  "keywords": [
    "github",
    "GraphQL",
    "schema"
  ],
  "scripts": {
    "lint": "eslint --fix src/**/*.ts",
    "precommit": "lint-staged && tsc",
    "prepublish": "tsc"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "lib/nestGraphQLEndpoint.js",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.3.6",
    "@graphql-tools/wrap": "^8.4.10",
    "@octokit/graphql-schema": "^10.36.0",
    "dataloader": "2.0.0",
    "node-fetch": "^3.2.3",
    "tslib": "~2.3.1",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/node-fetch": "2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.4.1",
    "sucrase": "^3.20.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "printWidth": 100,
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
